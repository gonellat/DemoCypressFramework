{
  "eslint.validate": [
    "javascript",
    "javascriptreact"
  ],
  "eslint.useFlatConfig": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.requireConfig": true,
  "files.eol": "\n",
  "cSpell.words": [
    "Automatio",
    "badeball",
    "democypressframework",
    "elif",
    "Gonella",
    "Happo",
    "mochawesome",
    "optin",
    "pytest",
    "winappdriver"
  ]
}
