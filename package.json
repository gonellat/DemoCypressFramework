{
  "name": "democypressframework",
  "version": "1.0.0",
  "description": "Cypress test automation framework",
  "author": "Tim Gonella",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/gonellat/DemoCypressFramework.git"
  },
  "homepage": "https://github.com/gonellat/DemoCypressFramework#readme",
  "bugs": {
    "url": "https://github.com/gonellat/DemoCypressFramework/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "test": "cypress run",
    "test:chrome": "cypress run --headed --browser chrome --env configEnv=local --reporter-options reportDir=cypress/reports/chrome",
    "test:edge": "cypress run --headed --browser edge --env configEnv=local --reporter-options reportDir=cypress/reports/edge",
    "test:headed": "cls && cypress run --headed --browser chrome --env configEnv=local",
    "clean:report": "rimraf cypress/reports/html/screenshots cypress/reports/html/videos",
    "test:headed:clean": "npm run test:headed && npm run clean:report",
    "test:parallel:browsers": "concurrently \"npm run test:chrome\" \"npm run test:edge\"",
    "lint": "eslint \"cypress/{e2e,support}/**/*.{js,ts}\"",
    "lint:fix": "eslint \"cypress/{e2e,support}/**/*.{js,ts}\" --fix",
    "format": "prettier \"**/*.{js,ts,json,md}\" --write",
    "fix:all": "npm run lint:fix && npm run format",
    "prepare": "husky install",
    "generate:locators": "node scripts/locator-extractor.js",
    "test:api": "cypress run --spec \"cypress/e2e/tests/api/**/*.cy.js\"",
    "merge:reports": "mochawesome-merge cypress/reports/json/*.json > cypress/reports/html/mochawesome.json",
    "report:open": "marge cypress/reports/html/mochawesome.json --reportDir cypress/reports/html --inline",
    "report:full": "npm run merge:reports && npm run report:open",
    "debug:cucumber": "cross-env DEBUG=@badeball/cypress-cucumber-preprocessor* cypress open"
  },
  "lint-staged": {
    "cypress/e2e/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "cypress/support/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.1.5",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cucumber/cucumber": "^12.1.0",
    "@faker-js/faker": "^9.9.0",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "cypress": "^14.5.4",
    "cypress-image-snapshot": "^4.0.1",
    "cypress-multi-reporters": "^2.0.5",
    "dotenv": "^17.2.1",
    "esbuild": "^0.25.9",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^52.0.4",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "mocha": "^11.7.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "multiple-cucumber-html-reporter": "^3.9.3",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.9.2",
    "vite": "^4.5.14"
  },
  "dependencies": {
    "minimist": "^1.2.8",
    "puppeteer": "^24.16.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/step-definitions/**/*.{js,ts}"
  }
}
