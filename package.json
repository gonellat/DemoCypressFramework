{
  "name": "democypressframework",
  "version": "1.0.0",
  "description": "Cypress test automation framework",
  "author": "Tim Gonella",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gonellat/DemoCypressFramework.git"
  },
  "type": "module",
  "scripts": {
    "test": "npx cypress run",
    "headTest": "npm run test --headed",
    "chromeTest": "npm run test --browser chrome",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [],
  "devDependencies": {
    "cypress": "^14.5.4",
    "cypress-cucumber-preprocessor": "^4.3.0",
    "cypress-dotenv": "^3.0.1",
    "cypress-mochawesome-reporter": "^4.0.0",
    "cypress-xpath": "^1.6.3",
    "eslint": "^9.29.0",
    "eslint-plugin-cypress": "^5.1.0",
    "mocha": "^11.7.1",
    "multiple-cucumber-html-reporter": "^3.9.3",
    "typescript": "^5.9.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/support/step_definitions",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/cucumber-json",
      "filePrefix": "cucumber-report-",
      "fileSuffix": ".json"
    }
  }
}